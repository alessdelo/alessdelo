<a-scene leap="vr: true" physics="debug: true">
      <a-entity camera="near: 0.01" look-controls position="0 1.5 0">
        <a-entity leap-hand="hand: right; enablePhysics: true"></a-entity>
        <a-entity leap-hand="hand: left; enablePhysics: true"></a-entity>
	

      </a-entity>
      
      	<a-box position="0.5 0.65 2.5" rotation="0 45 0" color="#4CC3D9" dynamic-body></a-box>
        <a-sphere position="0.5 -0.35 2.2" radius="0.2" color="#EF2D5E" dynamic-body ></a-sphere>
        <a-cylinder position="0.5 0.35 2.5" radius="0.2" height="0.7" color="#FFC65D" static-body></a-cylinder>
        <a-plane position="0 0 2" rotation="-90 0 0" width="2.5" height="2.5" color="#7BC8A4" static-body></a-plane>
        <a-sky color="#ECECEC"></a-sky>
      
      <!-- add a simple camera -->
      <!-- <a-entity camera spectator="true"></a-entity> -->
</a-scene>
